<?xml version='1.0' encoding='utf-8'?>
<widget id="kz.imbay.imbay" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>imbay</name>

	<description>
		imbay - social network
	</description>

	<author email="nurassyl.aldan@gmail.com">
		Nurasyl Aldan
	</author>

	<content src="index.html" />

	<icon src="res/icon/android/icon.png" />

	<plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
	<allow-navigation href="*" />
	<access origin="*" subdomains="true" />
	<allow-intent href="*" />

	<plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
	<preference name="StatusBarOverlaysWebView" value="true" />
	<preference name="StatusBarBackgroundColor" value="#337bb5" />
	<preference name="StatusBarStyle" value="lightcontent" />
</widget>

